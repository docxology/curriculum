    B -- Process 1 --> C((Predictive Model));
    C -- Hypothesis --> D{Compare Prediction};
    D -- Match --> E[Reinforce Prediction];
    D -- Mismatch --> F[Error Signal];
    F -- Adjust Model --> C;
    C -- Process 2 --> B;
