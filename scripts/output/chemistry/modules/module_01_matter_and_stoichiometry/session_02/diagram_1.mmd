graph TD
    A([Start: Atomic Nucleus]) --> B{Proton Count};
    B -- 1 --> C([Proton - +1 Charge]);
    B -- 0 --> D([Neutron - 0 Charge]);
    C --> E{Is Nucleus Stable?};
    D --> E;
    E -- Yes --> F([Stable Nucleus]);
    E -- No --> G{Induced Fission?};
    G -- Yes --> H([Fission Process]);
    G -- No --> I([Neutron Capture]);
    I --> J([New Nucleus]);
    J --> K{Is New Nucleus Stable?};
    K -- Yes --> L([Stable Nucleus]);
    K -- No --> M([Further Fission]);
    M --> N([Chain Reaction]);
    N --> O([Energy Release]);
    O --> P([Electron Emission]);
    P --> Q([Ion Formation]);
    Q --> R([Atomic Orbitals]);
    R --> S{Electron Interaction?};
    S -- Yes --> T([Photon Emission]);
    S -- No --> U([Electron Relaxation]);
    U --> V([Ground State]);
    V --> W{Quantum Tunneling?};
    W -- Yes --> X([Electron Transport]);
    W -- No --> Y([Electron Stability]);
    Y --> Z([Atomic Structure]);
    Z --> AA([Electrons - Negative Charge]);
    AA --> Z;
    C --> BB{Proton-Proton Chain?};
    BB -- Yes --> CC([Hydrogen Fusion]);
    BB -- No --> DD([Nuclear Decay]);
    DD --> EE([Heavier Nuclei]);
    EE --> FF{Further Decay?};
    FF -- Yes --> GG([Radioactive Decay]);
    FF -- No --> HH([Stable Isotope]);
    HH --> Z;
    Z --> Z;
    A --> Z;